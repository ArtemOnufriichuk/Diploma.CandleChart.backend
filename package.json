    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/socket.io": "^2.1.13",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "emailjs": "^3.4.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.33",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "socket.io": "^4.0.0",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.31",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0"
  }
}
