{
  "name": "trading",
  "version": "1.0.0",
  "description": "trading platform",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "rimraf build && tsc",
    "delete-db": "ts-node ./src/scripts/delete-database.ts",
    "create-db": "ts-node ./src/scripts/create-database.ts",
    "create-admin": "ts-node ./src/scripts/create-admin.ts",
    "full-restart-dev": "npm run delete-db && npm run create-db && npm run create-admin && ts-node src/index.ts",
    "start-prod": "cross-env NODE_ENV=production npm run create-db && cross-env NODE_ENV=production npm run create-admin && npm run build && cross-env NODE_ENV=production node build/index.js"
  },
  "author": "Jesus",
  "license": "ISC",
